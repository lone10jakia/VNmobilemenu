-- T·∫°o c·ª≠a s·ªï MEMAYBEO HUB
local Window = Fluent:CreateWindow({
    Title = "MEMAYBEO HUB - PvP Mode",
    SubTitle = "Kh√¥ng Key - Free 100%",
    TabWidth = 160,
    Size = UDim2.fromOffset(500, 300),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl,
})

local Tabs = {
    Main = Window:AddTab({ Title = "PvP Menu", Icon = "swords" })
}

