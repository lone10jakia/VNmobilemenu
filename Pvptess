    -------------------------------------------------------------------------
    -- SPIN MODE (Quay nhanh ng∆∞·ªùi ch∆°i)
    -------------------------------------------------------------------------

    local autoSpin = false

    AddToggle("Spin", false, function(v)
        autoSpin = v

        if v then
            task.spawn(function()
                while autoSpin and task.wait(0.03) do
                    local char = LocalPlayer.Character
                    if char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChildOfClass("Humanoid") then
                        char.Humanoid.AutoRotate = false
                        char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(999), 0)
                    end
                end
            end)
        else
            local char = LocalPlayer.Character
            if char and char:FindFirstChildOfClass("Humanoid") then
                char.Humanoid.AutoRotate = true
            end
        end
    end)

    -------------------------------------------------------------------------
    -- FIX LAG
    -------------------------------------------------------------------------

    AddToggle("FixLag v1", false, function(v)
        if v then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/getscript-vn/Api/refs/heads/main/Anti%20Lag"))()
        end
    end)

    -------------------------------------------------------------------------
    -- THAY ƒê·ªîI FOV
    -------------------------------------------------------------------------

    local oldFov = workspace.CurrentCamera.FieldOfView

    AddToggle("FOV 120", false, function(v)
        if v then
            workspace.CurrentCamera.FieldOfView = 120
        else
            workspace.CurrentCamera.FieldOfView = oldFov
        end
    end)

    print("MEMAYBEO HUB - PvP Mode Loaded!")
end -- end LoadPvpMenu

LoadPvpMenu()
Window:SelectTab(1)
-- KH·ªûI ƒê·ªòNG MENU PVP
LoadPvpMenu()

-- T·ª∞ ƒê·ªòNG CH·ªåN TAB
Window:SelectTab(1)
