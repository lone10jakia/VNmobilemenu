    -------------------------------------------------------------------------
    -- ESP TH√îNG TIN NG∆Ø·ªúI CH∆†I (T√™n + Level + PvP)
    -------------------------------------------------------------------------

    local espInfo = false

    AddToggle("ESP Th√¥ng Tin", false, function(v)
        espInfo = v

        if v then
            task.spawn(function()
                while espInfo and task.wait() do
                    for _, p in ipairs(Players:GetPlayers()) do
                        if p ~= LocalPlayer and p.Character then
                            local head = p.Character:FindFirstChild("Head")
                            if head and not head:FindFirstChild("PlayerInfo") then

                                local gui = Instance.new("BillboardGui")
                                gui.Name = "PlayerInfo"
                                gui.Size = UDim2.new(4,0,2,0)
                                gui.StudsOffset = Vector3.new(0,3,0)
                                gui.AlwaysOnTop = true
                                gui.Parent = head

                                local name = Instance.new("TextLabel", gui)
                                name.Size = UDim2.new(1,0,0.5,0)
                                name.BackgroundTransparency = 1
                                name.TextColor3 = Color3.fromRGB(255,255,255)
                                name.TextStrokeTransparency = 0
                                name.Font = Enum.Font.SourceSansBold
                                name.TextScaled = true
                                name.Text = p.Name

                                local info = Instance.new("TextLabel", gui)
                                info.Size = UDim2.new(1,0,0.5,0)
                                info.Position = UDim2.new(0,0,0.5,0)
                                info.BackgroundTransparency = 1
                                info.TextColor3 = Color3.fromRGB(255,255,255)
                                info.TextStrokeTransparency = 0
                                info.Font = Enum.Font.SourceSansBold
                                info.TextScaled = true

                                local stats = p:FindFirstChild("stats")
                                local lv = stats and stats:FindFirstChild("Level") and stats.Level.Value or "N/A"
                                info.Text = "Level: " .. tostring(lv) .. " | PvP: " .. tostring(p.Character:GetAttribute("PvPRemain") or "OFF")
                            end
                        end
                    end
                end
            end)
        else
            for _, p in ipairs(Players:GetPlayers()) do
                if p.Character and p.Character:FindFirstChild("Head") then
                    local g = p.Character.Head:FindFirstChild("PlayerInfo")
                    if g then g:Destroy() end
                end
            end
        end
    end)

    -------------------------------------------------------------------------
    -- HEALTH MODE (T·ª± ƒë·ªông d√πng bƒÉng g·∫°c n·∫øu HP th·∫•p)
    -------------------------------------------------------------------------

    local autoHeal = false

    AddToggle("Health Mode", false, function(v)
        autoHeal = v

        if v then
            task.spawn(function()
                local lp = LocalPlayer
                local backpack = lp:WaitForChild("Backpack")
                local char = lp.Character or lp.CharacterAdded:Wait()
                local healName = "bƒÉng g·∫°c"

                local function FindTool(where, name)
                    for _, tool in ipairs(where:GetChildren()) do
                        if tool:IsA("Tool") and string.find(string.lower(tool.Name), string.lower(name)) then
                            return tool
                        end
                    end
                end

                while autoHeal and task.wait() do
                    local hum = char:FindFirstChild("Humanoid")
                    if hum and hum.Health < 75 then
                        local toolNow = char:FindFirstChildOfClass("Tool")
                        local heal = FindTool(backpack, healName)

                        if heal then
                            hum:EquipTool(heal)
                            task.wait(0.05)
                            if heal.Activate then heal:Activate() end
                            task.wait(1.2)
                            if toolNow then hum:EquipTool(toolNow) end
                        end
                    end
                end
            end)
        end
    end)

    -------------------------------------------------------------------------
    -- AUTO L·∫§Y BƒÇNG G·∫†C (M·ªü Inventory t·ª± ƒë·ªông)
    -------------------------------------------------------------------------

    local autoBandage = false

    AddToggle("Auto L·∫•y BƒÉng G·∫°c", false, function(v)
        autoBandage = v

        if v then
            task.spawn(function()
                while autoBandage and task.wait(0.3) do
                    local lp = LocalPlayer
                    local pg = lp:WaitForChild("PlayerGui")
                    local inv = pg:WaitForChild("Inventory")

                    inv.MainFrame.Visible = true
                    inv.Enabled = true
                    inv.MainFrame.Position = UDim2.new(10, 0, 0.555, 0)

                    local backpack = lp:FindFirstChild("Backpack")
                    local char = lp.Character or lp.CharacterAdded:Wait()

                    if not backpack:FindFirstChild("bƒÉng g·∫°c") and not char:FindFirstChild("bƒÉng g·∫°c") then
                        game:GetService("ReplicatedStorage")
                            .KnitPackages._Index["sleitnick_knit@1.7.0"]
                            .knit.Services.InventoryService.RE.updateInventory:FireServer("eue", "bƒÉng g·∫°c")
                    end
                end
            end)
        else
            LocalPlayer.PlayerGui.Inventory.MainFrame.Position = UDim2.new(0.5,0,0.5,0)
        end
    end)

    -------------------------------------------------------------------------
    -- AUTO NH·∫∂T GH·∫æ
    -------------------------------------------------------------------------

    local autoChair = false

    AddToggle("Instant Nh·∫∑t Gh·∫ø", false, function(v)
        autoChair = v

        if v then
            task.spawn(function()
                while autoChair and task.wait() do

                    local lp = LocalPlayer
                    local char = lp.Character
                    if not char or not char:FindFirstChild("HumanoidRootPart") then continue end

                    -- D·ªãch chuy·ªÉn t·∫•t c·∫£ gh·∫ø l√™n tr·ªùi
                    for _, c in pairs(workspace.Ghe:GetChildren()) do
                        if c.Name == "Chair" then
                            c.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,150,0)
                        end
                    end

                    -- Nh·∫∑t gh·∫ø
                    if not lp.Backpack:FindFirstChild("Gh·∫ø") and not char:FindFirstChild("Gh·∫ø") then
                        for _, c in pairs(workspace.Ghe:GetChildren()) do
                            if c.Name == "Chair" and c:FindFirstChild("hitbox") and c.hitbox:FindFirstChild("ClickDetector") then
                                fireclickdetector(c.hitbox.ClickDetector)
                            end
                        end
                    end
                end
            end)
        end
    end)

    -------------------------------------------------------------------------
    -- CH·ªåI GH·∫æ NO COOLDOWN
