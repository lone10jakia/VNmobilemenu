local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local LP = Players.LocalPlayer
local Char = LP.Character or LP.CharacterAdded:Wait()
local HRP = Char:WaitForChild("HumanoidRootPart")
local Hum = Char:WaitForChild("Humanoid")

-- c·∫≠p nh·∫≠t character khi respawn
LP.CharacterAdded:Connect(function(c)
    Char = c
    HRP = c:WaitForChild("HumanoidRootPart")
    Hum = c:WaitForChild("Humanoid")
end)

--=========================================================
-- FUNCTION: T√¨m ng∆∞·ªùi ch∆°i g·∫ßn nh·∫•t
--=========================================================

local function GetNearestPlayer(maxDistance)
    local nearest = nil
    local shortest = maxDistance or 200

    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LP and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local targetHRP = plr.Character.HumanoidRootPart
            local distance = (targetHRP.Position - HRP.Position).Magnitude

            if distance < shortest then
                shortest = distance
                nearest = plr
            end
        end
    end
    
    return nearest
end

--=========================================================
-- FUNCTION: Teleport nh·∫π (anti rubberband)
--=========================================================

local function SoftTP(pos)
    if HRP then
        HRP.CFrame = CFrame.new(pos)
    end
end

--=========================================================
-- FUNCTION: Auto Lock-On (camera + body align)
--=========================================================

local LockTarget = nil
local Locking = false

local function EnableLock(plr)
    if plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
        LockTarget = plr
        Locking = true
    end
end

local function DisableLock()
    LockTarget = nil
    Locking = false
end

-- H√†m ch·∫°y lock-on
RunService.RenderStepped:Connect(function()
    if Locking and LockTarget and LockTarget.Character then
        local T_HRP = LockTarget.Character:FindFirstChild("HumanoidRootPart")
        if T_HRP then
            workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, T_HRP.Position)
        end
    end
end)

--=========================================================
-- FUNCTION: Auto Attack (click queue)
--=========================================================

local VU = game:GetService("VirtualUser")
local AutoAttack = false

local function DoAttack()
    if AutoAttack then
        VU:CaptureController()
        VU:ClickButton1(Vector2.new(9999, 9999))
    end
end

task.spawn(function()
    while task.wait(0.15) do
        if AutoAttack then
            DoAttack()
        end
    end
end)

--=========================================================
-- FUNCTION: Nh·∫£y spam (bunny hop)
--=========================================================

local AutoJump = false
task.spawn(function()
    while task.wait(0.05) do
        if AutoJump and Hum then
            Hum.Jump = true
        end
    end
end)

--=========================================================
-- FUNCTION: ƒêi xuy√™n t∆∞·ªùng (noclip)
--=========================================================

local NoClip = false
RunService.Stepped:Connect(function()
    if NoClip and Char then
        for _, v in pairs(Char:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end
end)

