SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({})
InterfaceManager:SetFolder("MemaybeoHub/UI")
SaveManager:SetFolder("MemaybeoHub/Config")

InterfaceManager:BuildInterfaceSection(SettingsTab)
SaveManager:BuildConfigSection(SettingsTab)

SaveManager:LoadAutoloadConfig()
--=========================================================
-- MEMAYBEO HUB ‚Äî MOVEMENT / UTILITY FEATURES
--=========================================================

local MovementTab = Window:AddTab({
    Title = "Di chuy·ªÉn",
    Icon = "zap"
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

--=========================================================
-- N O C L I P  (ƒêi xuy√™n t∆∞·ªùng)
--=========================================================

local NoClip = false

MovementTab:AddToggle("NoClipToggle", {
    Title = "ƒêi xuy√™n t∆∞·ªùng (NoClip)",
    Default = false,
    Callback = function(v)
        NoClip = v
    end
})

RunService.Stepped:Connect(function()
    if NoClip and LocalPlayer.Character then
        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

--=========================================================
-- I N V I S I B I L I T Y  (Bi·∫øn m·∫•t)
--=========================================================

local Invisible = false
local StoredParts = {}

local function SetInvisible(state)
    local char = LocalPlayer.Character
    if not char then return end

    if state then
        for _, part in ipairs(char:GetDescendants()) do
            if part:IsA("BasePart") and part.Transparency < 1 then
                StoredParts[part] = part.Transparency
                part.Transparency = 1
                if part:FindFirstChild("face") then
                    part.face.Transparency = 1
                end
            end
        end
    else
        for part, original in pairs(StoredParts) do
            if part then
                part.Transparency = original
            end
        end
        StoredParts = {}
    end
end

MovementTab:AddToggle("InvisibleToggle", {
    Title = "T√†ng h√¨nh (Invisibility)",
    Default = false,
    Callback = function(v)
        Invisible = v
        SetInvisible(v)
    end
})

--=========================================================
-- TELEPORT ƒê·∫æN V·ªä TR√ç
--=========================================================

local function TP(pos)
    local char = LocalPlayer.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.CFrame = CFrame.new(pos)
    end
end

MovementTab:AddButton({
    Title = "Quay v·ªÅ Safe Zone",
    Callback = function()
        TP(Vector3.new(0, 10, 0)) -- tu·ª≥ ch·ªânh n·∫øu c·∫ßn
    end
})

--=========================================================
-- TELEPORT T·ªöI NG∆Ø·ªúI CH∆†I
--=========================================================

local PlayerList = {}

for _, p in pairs(Players:GetPlayers()) do
    table.insert(PlayerList, p.Name)
end

MovementTab:AddDropdown("TPPlayer", {
    Title = "Teleport t·ªõi ng∆∞·ªùi ch∆°i",
    Values = PlayerList,
    Multi = false,
    Callback = function(name)
        local target = Players:FindFirstChild(name)
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            TP(target.Character.HumanoidRootPart.Position + Vector3.new(0, 3, 0))
        end
    end
})

--=========================================================
-- F A K E   L A G  (gi·∫≠t lag ƒë·ªïi v·ªã tr√≠)
--=========================================================

local FakeLag = false
local LastCF = nil

MovementTab:AddToggle("FakeLagToggle", {
    Title = "Fake Lag (gi·∫≠t l√πi v·ªã tr√≠)",
    Default = false,
    Callback = function(v)
        FakeLag = v
        if v then
            task.spawn(function()
                while FakeLag do
                    local HRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if HRP then
                        LastCF = HRP.CFrame
                        HRP.Anchored = true
                    end
                    task.wait(0.35)
                    if HRP then
                        HRP.Anchored = false
                        HRP.CFrame = LastCF
                    end
                    task.wait(0.15)
                end
            end)
        end
    end
})

--=========================================================
-- ANTI SLOW / ANTI STUN
--=========================================================

local AntiSlow = false

MovementTab:AddToggle("AntiSlowToggle", {
    Title = "Anti Slow / Anti Stun",
    Default = true,
    Callback = function(v)
        AntiSlow = v
    end
})

RunService.Heartbeat:Connect(function()
    if AntiSlow and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = 16
        LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

--=========================================================
-- INFINITE JUMP
--=========================================================

local InfJump = false

MovementTab:AddToggle("InfJumpToggle", {
    Title = "Nh·∫£y v√¥ h·∫°n",
    Default = false,
    Callback = function(v)
        InfJump = v
    end
})

UIS.JumpRequest:Connect(function()
    if InfJump and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

--=========================================================
-- HIGH JUMP
--=========================================================

MovementTab:AddButton({
    Title = "High Jump",
    Callback = function()
        local HRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if HRP then
            HRP.Velocity = Vector3.new(0, 100, 0)
        end
    end
})
--=========================================================
-- MEMAYBEO HUB ‚Äî LOADER / GUI FINAL
--=========================================================

-- GUI Variables
local CoreGui = game:GetService("CoreGui")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Loader Settings
local LoaderTitle = "MEMAYBEO HUB"
local LoaderMessage = "ƒêang kh·ªüi ch·∫°y..."
local LoaderDuration = 2.5 -- gi√¢y

-- Anti Double UI
if CoreGui:FindFirstChild(LoaderTitle) then
    CoreGui[LoaderTitle]:Destroy()
end

-- ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = LoaderTitle
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = CoreGui

-- Main Frame
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 400, 0, 200)
MainFrame.Position = UDim2.new(0.5, -200, 0.5, -100)
MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
MainFrame.BorderSizePixel = 0
MainFrame.Parent = ScreenGui
MainFrame.Visible = false -- ·∫©n l√∫c kh·ªüi t·∫°o

-- UICorner cho ƒë·∫πp
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 12)
UICorner.Parent = MainFrame

-- Loader Text
local LoaderLabel = Instance.new("TextLabel")
LoaderLabel.Size = UDim2.new(1, -20, 0, 50)
LoaderLabel.Position = UDim2.new(0, 10, 0, 10)
LoaderLabel.Text = LoaderMessage
LoaderLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
LoaderLabel.BackgroundTransparency = 1
LoaderLabel.Font = Enum.Font.GothamBold
LoaderLabel.TextSize = 24
LoaderLabel.Parent = MainFrame

-- Close / Hide Button
local HideButton = Instance.new("TextButton")
HideButton.Size = UDim2.new(0, 30, 0, 30)
HideButton.Position = UDim2.new(1, -40, 0, 10)
HideButton.Text = "√ó"
HideButton.Font = Enum.Font.GothamBold
HideButton.TextSize = 24
HideButton.TextColor3 = Color3.fromRGB(255, 255, 255)
HideButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
HideButton.BorderSizePixel = 0
HideButton.Parent = MainFrame

local ButtonCorner = Instance.new("UICorner")
ButtonCorner.CornerRadius = UDim.new(0, 6)
ButtonCorner.Parent = HideButton

HideButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- Show Loader
ScreenGui.Enabled = true
MainFrame.Visible = true

-- Loader Animation
task.spawn(function()
    local t = 0
    while t < LoaderDuration do
        t += RunService.Heartbeat:Wait()
        LoaderLabel.TextTransparency = 1 - (t / LoaderDuration)
    end
    LoaderLabel.Text = "MEMAYBEO HUB s·∫µn s√†ng! üéâ"
    wait(1.2)
    MainFrame.Visible = true
end)

-- Anti Crash / Safe GUI
RunService:BindToRenderStep("AntiCrashGUI", 1000, function()
    if not ScreenGui.Parent then
        ScreenGui.Parent = CoreGui
    end
end)

-- Optional: Keybind ƒë·ªÉ Show/Hide to√†n b·ªô menu
local UserInputService = game:GetService("UserInputService")
local toggleKey = Enum.KeyCode.RightControl -- b·∫•m Ctrl ph·∫£i

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == toggleKey then
        MainFrame.Visible = not MainFrame.Visible
    end
end)
