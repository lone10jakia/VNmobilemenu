--=========================================================
-- MEMAYBEO HUB - ADVANCED PvP COMBAT
--=========================================================

local CombatSettings = {
    TeleportDistance = 6,
    TeleportSpeed = 0.15,
    AimSmoothness = 0.25,
    DashCooldown = 1.2,
    HitboxSize = 7
}

local AutoTP = false
local AimAssist = false
local AutoDash = false
local ExpandHitbox = false

--=========================================================
-- FUNCTION: Teleport gi·ªØ kho·∫£ng c√°ch chu·∫©n
--=========================================================

local function TP_ToTarget(target)
    if not target or not target.Character then return end
    local hrp = target.Character:FindFirstChild("HumanoidRootPart")
    if not hrp or not HRP then return end

    local dir = (hrp.Position - HRP.Position).Unit
    local newpos = hrp.Position - dir * CombatSettings.TeleportDistance

    HRP.CFrame = HRP.CFrame:Lerp(
        CFrame.new(newpos, hrp.Position),
        CombatSettings.TeleportSpeed
    )
end

--=========================================================
-- AUTO TELEPORT COMBAT FOLLOW
--=========================================================

task.spawn(function()
    while task.wait() do
        if AutoTP and LockTarget and LockTarget.Character then
            TP_ToTarget(LockTarget)
        end
    end
end)

--=========================================================
-- AIM ASSIST (camera xo√°y m∆∞·ª£t)
--=========================================================

RunService.RenderStepped:Connect(function()
    if AimAssist and LockTarget and LockTarget.Character then
        local targetHRP = LockTarget.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            local cam = workspace.CurrentCamera
            cam.CFrame = cam.CFrame:Lerp(
                CFrame.new(cam.CFrame.Position, targetHRP.Position),
                CombatSettings.AimSmoothness
            )
        end
    end
end)

--=========================================================
-- AUTO DASH N√â SKILL
--=========================================================

local lastDash = 0

local function DoDash()
    if tick() - lastDash < CombatSettings.DashCooldown then return end
    lastDash = tick()

    task.spawn(function()
        for i = 1, 4 do
            Hum.Jump = true
            HRP.Velocity = Vector3.new(
                math.random(-90, 90),
                math.random(40, 90),
                math.random(-90, 90)
            )
            task.wait(0.05)
        end
    end)
end

task.spawn(function()
    while task.wait(0.1) do
        if AutoDash and LockTarget then
            local dist = (HRP.Position - LockTarget.Character.HumanoidRootPart.Position).Magnitude
            if dist < 9 then
                DoDash()
            end
        end
    end
end)

--=========================================================
-- HITBOX EXPANDER
--=========================================================

task.spawn(function()
    while task.wait(1) do
        if ExpandHitbox then
            for _, plr in pairs(Players:GetPlayers()) do
                if plr ~= LP and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                    local hb = plr.Character.HumanoidRootPart
                    hb.Size = Vector3.new(
                        CombatSettings.HitboxSize,
                        CombatSettings.HitboxSize,
                        CombatSettings.HitboxSize
                    )
                    hb.Transparency = 0.8
                    hb.CanCollide = false
                end
            end
        end
    end
end)

