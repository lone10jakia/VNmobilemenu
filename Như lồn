--=========================================================
-- ANTI KICK (c∆° b·∫£n)
--=========================================================

local AntiKick = false

SettingsTab:AddToggle("AntiKickEnable", {
    Title = "Anti Kick (basic)",
    Description = "Ch·∫∑n 80% kick th·ªß c√¥ng / remote",
    Default = false,
    Callback = function(v)
        AntiKick = v
        if v then
            local mt = getrawmetatable(game)
            local old = mt.__namecall
            setreadonly(mt, false)

            mt.__namecall = newcclosure(function(self, ...)
                local method = getnamecallmethod()
                if AntiKick and tostring(method) == "Kick" then
                    return nil
                end
                return old(self, ...)
            end)

            setreadonly(mt, true)
        end
    end
})

--=========================================================
-- FPS UNLOCK + BOOST GAME
--=========================================================

local BoostOn = false

SettingsTab:AddToggle("FPSBOOST", {
    Title = "Boost FPS c·ª±c m·∫°nh",
    Default = false,
    Callback = function(v)
        BoostOn = v

        if v then
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") then
                    obj.CastShadow = false
                elseif obj:IsA("Decal") or obj:IsA("Texture") then
                    obj.Transparency = 1
                elseif obj:IsA("ParticleEmitter") or obj:IsA("Trail") then
                    obj.Enabled = false
                end
            end
            setfpscap(240)
        else
            setfpscap(60)
        end
    end
})

--=========================================================
-- T·ªêI ∆ØU B·ªò NH·ªö (deep cleaner)
--=========================================================

SettingsTab:AddButton({
    Title = "D·ªçn b·ªô nh·ªõ",
    Description = "D·ªçn r√°c + Clear connection",
    Callback = function()
        collectgarbage("collect")

        for _, v in pairs(getgc(true)) do
            if type(v) == "function" and islclosure(v) then
                debug.setconstant(v,1,nil)
            end
        end

        Window:Notify({
            Title = "MEMAYBEO HUB",
            Content = "ƒê√£ d·ªçn b·ªô nh·ªõ!",
            Duration = 3
        })
    end
})

--=========================================================
-- L∆ØU & T·ª∞ LOAD CONFIG
