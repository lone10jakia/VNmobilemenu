--=========================================================
-- MEMAYBEO HUB - ESP + HUD CHI·∫æN ƒê·∫§U
--=========================================================

local ESP_Enabled = false
local TargetHUD_Enabled = true
local Warning_Enabled = true

local Drawings = {}

--=========================================================
-- FUNCTION: T·∫°o text 2D
--=========================================================

local function CreateText()
    local t = Drawing.new("Text")
    t.Size = 15
    t.Center = true
    t.Outline = true
    t.Color = Color3.new(1,1,1)
    return t
end

--=========================================================
-- ESP PLAYER
--=========================================================

task.spawn(function()
    while task.wait() do
        if not ESP_Enabled then
            for _, d in pairs(Drawings) do
                d:Remove()
            end
            Drawings = {}
        else
            for _, plr in pairs(Players:GetPlayers()) do
                if plr ~= LP and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                    
                    -- t·∫°o text n·∫øu ch∆∞a c√≥
                    if not Drawings[plr] then
                        Drawings[plr] = CreateText()
                    end
                    
                    local d = Drawings[plr]
                    local hrp = plr.Character.HumanoidRootPart
                    local pos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(hrp.Position)
                    
                    if onScreen then
                        d.Visible = true
                        d.Position = Vector2.new(pos.X, pos.Y)
                        d.Text = plr.Name .. " | HP: " .. math.floor(plr.Character.Humanoid.Health)
                        
                        if LockTarget == plr then
                            d.Color = Color3.fromRGB(255, 50, 50) -- ƒë·ªè khi l√† target
                        else
                            d.Color = Color3.fromRGB(255, 255, 255)
                        end
                    else
                        d.Visible = false
                    end
                end
            end
        end
    end
end)

--=========================================================
-- HUD M·ª§C TI√äU (hi·ªán g√≥c m√†n h√¨nh)
--=========================================================

local TargetHUD = Drawing.new("Text")
TargetHUD.Size = 18
TargetHUD.Position = Vector2.new(40, 300)
TargetHUD.Color = Color3.new(1,1,1)
TargetHUD.Outline = true
TargetHUD.Center = false
TargetHUD.Visible = true

task.spawn(function()
    while task.wait(0.1) do
        if TargetHUD_Enabled and LockTarget and LockTarget.Character then
            local hum = LockTarget.Character:FindFirstChild("Humanoid")
            if hum then
                TargetHUD.Visible = true
                TargetHUD.Text =
                    "üéØ TARGET: " .. LockTarget.Name ..
                    "\n‚ù§Ô∏è HP: " .. math.floor(hum.Health) ..
                    "\nüìè Distance: " .. math.floor((HRP.Position - LockTarget.Character.HumanoidRootPart.Position).Magnitude)
            end
        else
            TargetHUD.Visible = false
        end
    end
end)

--=========================================================
-- C·∫¢NH B√ÅO KHI B·ªä NG∆Ø·ªúI KH√ÅC LOCK
--=========================================================

local WarningText = Drawing.new("Text")
WarningText.Size = 22
WarningText.Color = Color3.fromRGB(255, 50, 50)
WarningText.Outline = true
WarningText.Center = true
WarningText.Position = Vector2.new(workspace.CurrentCamera.ViewportSize.X/2, 120)
WarningText.Visible = false

local function SomeoneLookingAtYou(plr)
    if not plr or not plr.Character then return false end
    if not LP.Character then return false end

    local theirCam = plr.Character:FindFirstChild("HumanoidRootPart")
    local myHRP = HRP

    if theirCam and myHRP then
        local dot = (myHRP.Position - theirCam.Position).Unit:Dot(theirCam.CFrame.LookVector)
        return dot > 0.9 -- 0.9 = nh√¨n th·∫≥ng v√†o b·∫°n
    end
    return false
end

task.spawn(function()
    while task.wait(0.2) do
        if Warning_Enabled then
            local danger = false

            for _, plr in pairs(Players:GetPlayers()) do
                if plr ~= LP then
                    if SomeoneLookingAtYou(plr) then
                        danger = true
                        break
                    end
                end
            end

            WarningText.Visible = danger

            if danger then
                WarningText.Text = "‚ö†Ô∏è C√ì NG∆Ø·ªúI ƒêANG NH√åN B·∫†N! ‚ö†Ô∏è"
            end
        else
            WarningText.Visible = false
        end
    end
end)

